# Logo

class exports.LogoLayer extends SVGLayer
	constructor: (@options={}) ->
		
		_.defaults @options,
			opacity: 0.5
			handler: null
			svg: getLogo("FFF")
		
		super @options
		@style = cursor: "pointer"
		@showHint = -> ;
		
		@.onMouseOver @Hover
		@.onMouseOut @HoverOff
	
	@define 'handler',
		set: (value) -> @on(Events.Tap, value)
	
	Hover: =>
		@opacity = 0.8
	HoverOff: =>
		@opacity = 0.5



